<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.phonegap.example"
        versionCode = "10" 
        version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>PhoneGap Vibration</name>

    <description>
        Ejemplo para utilizar Plugins Para Teclas de Piano
    </description>

    <preference name="phonegap-version" value="3.1.0" />
	<preference name="orientation" value="landscape"/>
    <author href="https://build.phonegap.com" email="support@phonegap.com">
        Brenda Juana Orta Arias
    </author>
	

		<gap:plugin name="org.apache.cordova.vibration" version="0.3.0" />
<gap:plugin name="com.rjfun.cordova.plugin.lowlatencyaudio" version="1.0" />  <!-- aqui agregamos el plugin de alguien mas -->

</widget>